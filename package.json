{
  "name": "stratix",
  "version": "0.9.0",
  "private": true,
  "type": "module",
  "description": "Modern TypeScript framework with Domain-Driven Design, hexagonal architecture, and native AI agent support",
  "author": "P. Andr√©s Carvajal <causticrez@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "dependencies": {
    "awilix": "12.0.5",
    "yaml": "2.8.2"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "prettier": "3.7.4",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vitest": "4.0.16"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "pnpm@10.28.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "framework",
    "domain-driven-design",
    "ddd",
    "hexagonal-architecture",
    "clean-architecture",
    "cqrs",
    "event-sourcing",
    "ai-agents",
    "dependency-injection",
    "enterprise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stratix-dev/stratix.git"
  },
  "bugs": {
    "url": "https://github.com/stratix-dev/stratix/issues"
  },
  "homepage": "https://stratixdev.com"
}
