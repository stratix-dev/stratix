{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Stratix Framework",
  "entryPoints": [
    "packages/core/src/index.ts",
    "packages/runtime/src/index.ts",
    "packages/testing/src/index.ts",
    "packages/cli/src/index.ts",
    "packages/providers/ai/openai/src/index.ts",
    "packages/providers/ai/anthropic/src/index.ts",
    "packages/providers/di/awilix/src/index.ts",
    "packages/providers/validation/zod/src/index.ts",
    "packages/plugins/database/postgres/src/index.ts",
    "packages/plugins/database/mongodb/src/index.ts",
    "packages/plugins/database/redis/src/index.ts",
    "packages/plugins/http/fastify/src/index.ts",
    "packages/plugins/http/client/src/index.ts",
    "packages/plugins/messaging/rabbitmq/src/index.ts",
    "packages/plugins/security/auth/src/index.ts",
    "packages/plugins/observability/opentelemetry/src/index.ts",
    "packages/libraries/errors/src/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "tsconfig": "tsconfig.typedoc.json",
  "out": "docs/api",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "**/node_modules/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "readme": "README.md",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Runtime",
    "AI Agents",
    "Plugins",
    "Providers",
    "Testing",
    "CLI",
    "*"
  ],
  "sort": [
    "source-order"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  },
  "plugin": [],
  "theme": "default",
  "lightHighlightTheme": "light-plus",
  "darkHighlightTheme": "dark-plus",
  "customCss": "./typedoc-theme.css",
  "markedOptions": {},
  "basePath": ".",
  "githubPages": true,
  "hideGenerator": false,
  "searchInComments": true,
  "cleanOutputDir": true,
  "titleLink": "https://stratixdev.com/",
  "navigationLinks": {
    "GitHub": "https://github.com/stratix-dev/stratix",
    "Documentation": "https://stratix-dev.github.io/stratix/",
    "NPM": "https://www.npmjs.com/org/stratix"
  },
  "sidebarLinks": {
    "Getting Started": "https://stratix-dev.github.io/stratix/docs/getting-started/quick-start"
  },
  "includeVersion": true,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/stratix-dev/stratix/blob/{gitRevision}/{path}#L{line}",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}