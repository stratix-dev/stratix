{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Stratix Framework",
  "entryPoints": [
    "packages/core/src/index.ts",
    "packages/runtime/src/index.ts",
    "packages/testing/src/index.ts",
    "packages/ai/providers/anthropic/src/index.ts",
    "packages/ai/providers/openai/src/index.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "docs",
  "readme": "README.md",
  "tsconfig": "./tsconfig.docs.json",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeExternals": true,
  "exclude": [
    "**/__tests__/**",
    "**/test/**",
    "**/*.test.ts",
    "**/vitest.config.ts",
    "**/node_modules/**"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  },
  "categorizeByGroup": true,
  "groupOrder": [
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Functions",
    "Variables",
    "*"
  ],
  "categoryOrder": [
    "Domain Primitives",
    "Result Pattern",
    "Value Objects",
    "Messaging (CQRS)",
    "AI Agents",
    "Dependency Injection",
    "Runtime & Application",
    "Testing",
    "Configuration",
    "Validation",
    "Infrastructure",
    "Patterns",
    "*"
  ],
  "sort": ["source-order"],
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true
  },
  "includeVersion": true,
  "searchInComments": true,
  "plugin": []
}
