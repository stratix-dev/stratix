{
  "name": "@stratix/msg-rabbitmq",
  "version": "0.7.3",
  "description": "RabbitMQ extension for Stratix framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run || exit 0",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stratix",
    "rabbitmq",
    "messaging",
    "queue",
    "event-bus"
  ],
  "author": "P. Andr√©s Carvajal <causticrez@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.4",
    "@stratix/core": "workspace:^"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "typescript": "^5.7.2",
    "vitest": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stratix-dev/stratix.git",
    "directory": "packages/plugins/messaging/rabbitmq"
  },
  "homepage": "https://stratix-dev.github.io/stratix/",
  "bugs": {
    "url": "https://github.com/stratix-dev/stratix/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
