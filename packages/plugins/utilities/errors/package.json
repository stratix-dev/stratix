{
  "name": "@stratix/errors",
  "version": "0.4.2",
  "description": "Error handling utilities for Stratix framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run || exit 0",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stratix",
    "errors",
    "error-handling"
  ],
  "author": "Stratix Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stratix-dev/stratix.git",
    "directory": "packages/plugins/utilities/errors"
  },
  "dependencies": {
    "@stratix/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://stratix-dev.github.io/stratix/"
}
