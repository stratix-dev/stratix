{
  "name": "@stratix/http-client",
  "version": "0.7.6",
  "description": "HTTP client plugin for Stratix framework using Axios",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stratix",
    "http",
    "client",
    "axios",
    "plugin",
    "rest",
    "api"
  ],
  "author": "Stratix Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stratix-dev/stratix.git",
    "directory": "packages/plugins/http/client"
  },
  "homepage": "https://stratix-dev.github.io/stratix/",
  "dependencies": {
    "@stratix/core": "workspace:^",
    "@stratix/runtime": "workspace:^",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
