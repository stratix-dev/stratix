{
  "name": "@stratix/core",
  "version": "0.3.0",
  "description": "Stratix core - DDD primitives, abstractions, and interfaces",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ddd",
    "domain-driven-design",
    "entity",
    "value-object",
    "aggregate-root",
    "primitives",
    "abstractions",
    "interfaces",
    "dependency-injection",
    "cqrs",
    "event-bus",
    "typescript",
    "hexagonal-architecture",
    "clean-architecture",
    "ai-agents"
  ],
  "author": "P. Andr√©s Carvajal <causticrez@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pcarvajal/stratix.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/pcarvajal/stratix#readme",
  "bugs": {
    "url": "https://github.com/pcarvajal/stratix/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
