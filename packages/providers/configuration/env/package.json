{
  "name": "@stratix/config-env",
  "version": "0.7.5",
  "description": "Environment-based configuration provider for Stratix",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run || exit 0",
    "test:watch": "vitest",
    "lint": "eslint src --ignore-pattern '**/*.test.ts' --ignore-pattern '**/__tests__/**'",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stratix",
    "configuration",
    "config",
    "environment",
    "env",
    "dotenv"
  ],
  "author": "Stratix Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stratix-dev/stratix.git",
    "directory": "packages/providers/configuration/env"
  },
  "dependencies": {
    "@stratix/core": "workspace:^",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://stratix-dev.github.io/stratix/"
}
